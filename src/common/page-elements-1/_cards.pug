.morgage-wrapper
    .mortgage__block-image
        .card-image-transform
            img(src='common/images/kard.png')
    .mortgage-intro.container
        .mortgage-intro__block
            .mortgage__block-content
                h6.title-block__description.grey-color Новая кредитная карта
                h1.title-block__title #ВСЁСРАЗУ
                p.block-content__readme.grey-color Оплачивайте покупки и накапливайте мили для премиальных перелётов и повышения класса обслуживания по программе Miles&More
                .block-content__description
                    .property
                        .property__block
                            h3.property__title 5%
                            p.property__description cashback на все покупки по карте

                    .property
                        .property__block
                            h3.property__title 50
                                span.small-text дней
                            p.property__description беспроцентный льготный период

                    .property
                        .property__block
                            h3.property__title image
                            p.property__description индивидуальный дизайн карты

                .mortgage__block-buttons
                    //a.button(href='#23') Оставить заявку
                    +btn('Оформить карту', '#23')
                    +btn-text('Подробнее', '#23')

